import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        int i=1;
       Scanner sc= new Scanner(System.in);
       do{
        String n=sc.nextLine();
        System.out.println(i+" "+n);
        i++;
       }while(sc.hasNext());
    }
}
